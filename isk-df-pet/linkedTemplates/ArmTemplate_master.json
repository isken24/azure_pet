{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "isk-df-pet"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"demoOutputSQL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'demoOutputSQL'"
		},
		"outputSQL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'outputSQL'"
		},
		"Trigger_bgx_properties_pipeline_parameters_windowStart": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"Trigger_bgx_properties_pipeline_parameters_windowEnd": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "isk-df-pet_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"demoOutputSQL_connectionString": {
						"value": "[parameters('demoOutputSQL_connectionString')]"
					},
					"outputSQL_connectionString": {
						"value": "[parameters('outputSQL_connectionString')]"
					},
					"Trigger_bgx_properties_pipeline_parameters_windowStart": {
						"value": "[parameters('Trigger_bgx_properties_pipeline_parameters_windowStart')]"
					},
					"Trigger_bgx_properties_pipeline_parameters_windowEnd": {
						"value": "[parameters('Trigger_bgx_properties_pipeline_parameters_windowEnd')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}